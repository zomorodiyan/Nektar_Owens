<?xml version="1.0" encoding="utf-8"?>

<NEKTAR>

  <EXPANSIONS>
    <E COMPOSITE="C[0]" NUMMODES="7" FIELDS="u,v,Txx,Txy,Tyy,Na,p" TYPE="MODIFIED" />
  </EXPANSIONS>

  <CONDITIONS>
    <SOLVERINFO>
      <I PROPERTY="SolverType"  VALUE="VelocityCorrectionScheme"/>
      <I PROPERTY="EQTYPE" VALUE="UnsteadyViscoElastic"/>
      <I PROPERTY="VISELTYPE" VALUE="HomogeneousBloodModel"/>
      <I PROPERTY="VISCOELASTICTREATMENT" VALUE="ImplicitExplicit"/>
      <I PROPERTY="AdvectionForm" VALUE="Convective"/>
      <I PROPERTY="Projection" VALUE="Galerkin"/>
      <I PROPERTY="TimeIntegrationMethod" VALUE="IMEXOrder3"/>
<!--      <I PROPERTY="HOMOGENEOUS" VALUE="1D"/> -->
    </SOLVERINFO>

    <PARAMETERS>
      <P> TimeStep      = 0.0001     </P>
      <P> NumSteps      =10000      </P>
      <P> IO_CheckSteps = 10000       </P>
      <P> IO_InfoSteps  = 1       </P>
      <P> Kinvis        = 1.0         </P>
      <P> Reynolds        = 1.0       </P>
      <P> Weissenberg     =0.1       </P>
      <P> Rmu        = 1/9        </P>
      <P> HomModesZ     = 20          </P> 
      <P> LZ            = 1.0        </P>
    </PARAMETERS>

    <VARIABLES>
      <V ID="0"> u </V> 
      <V ID="1"> v </V>
      <V ID="2"> Txx </V> 
      <V ID="3"> Txy </V> 
      <V ID="4"> Tyy </V> 
      <V ID="5"> Na </V> 
      <V ID="6"> p </V> 

    </VARIABLES>

    <BOUNDARYREGIONS>
      <B ID="0"> C[1] </B>
      <B ID="1"> C[2] </B>
      <B ID="2"> C[3] </B>
    </BOUNDARYREGIONS>

    <BOUNDARYCONDITIONS>
      <REGION REF="0">
 
<!--        <D VAR="u" VALUE="-12/11*TimeStep*Kinvis" />-->
        <D VAR="u" VALUE="0" /> 
        <D VAR="v" VALUE="0" />
        <D VAR="Txx"  VALUE="2*Reynolds*Reynolds*Kinvis*Kinvis*Weissenberg*(1-Rmu)*(1-2*y)*(1-2*y)" /> 
        <D VAR="Txy"  VALUE="Reynolds*Kinvis*(1-Rmu)*(1-2*y)" />
        <D VAR="Tyy" VALUE="0" />
        <D VAR="Na" VALUE="0" />
        <N VAR="p" VALUE="0"/>  

      </REGION>

      <REGION REF="1">
<!--        <D VAR="u" VALUE="Kinvis*Reynolds*y*(1-y)-12/11*TimeStep*Kinvis" /> -->
         <D VAR="u" VALUE="Kinvis*Reynolds*y*(1-y)" />
         <D VAR="v" VALUE="0" />

        <D VAR="Txx" VALUE="2*Reynolds*Reynolds*Kinvis*Kinvis*Weissenberg*(1-Rmu)*(1-2*y)*(1-2*y)" />
        <D VAR="Txy" VALUE="Reynolds*Kinvis*(1-Rmu)*(1-2*y)" />
 
        <D VAR="Tyy" VALUE="0" />

        <D VAR="Na" VALUE="0" />
<!--        <D VAR="p" VALUE="-12/11*TimeStep*Kinvis*(x-1)" />  // High Order Pressure BC -->
        <N VAR="p" VALUE="2.0*Kinvis" />  // High Order Pressure BC

      </REGION>

      <REGION REF="2">
        <N VAR="u" VALUE="0" />
        <D VAR="v" VALUE="0" />

        <N VAR="Txx" VALUE="0" />
        <N VAR="Txy" VALUE="0" />

        <N VAR="Tyy" VALUE="0" />

        <D VAR="Na" VALUE="0" />

        <D VAR="p"   VALUE="-2.0*Kinvis*(x-1)" />

      </REGION>

    </BOUNDARYCONDITIONS>

   <FUNCTION NAME="InitialConditions">
     <E VAR="u" VALUE="Kinvis*Reynolds*y*(1-y)" />
     <E VAR="v" VALUE="0.0" />
     <E VAR="Txx" VALUE="2*Reynolds*Reynolds*Kinvis*Kinvis*Weissenberg*(1-Rmu)*(1-2*y)*(1-2*y)" />
     <E VAR="Txy" VALUE="Reynolds*Kinvis*(1-Rmu)*(1-2*y)" />
     <E VAR="Tyy" VALUE="0.0" />
     <E VAR="Na" VALUE="0" />
     <E VAR="p" VALUE="-2*Kinvis*(x-1)" />
   </FUNCTION>

    <FUNCTION NAME="ExactSolution">
      <E VAR="u" VALUE="Kinvis*Reynolds*y*(1-y)" />
      <E VAR="v" VALUE="0" />
      <E VAR="Txx" VALUE="2*Reynolds*Reynolds*Kinvis*Kinvis*Weissenberg*(1-Rmu)*(1-2*y)*(1-2*y)" />
      <E VAR="Txy" VALUE="Reynolds*Kinvis*(1-Rmu)*(1-2*y)" /> 
      <E VAR="Tyy" VALUE="0" /> 
      <E VAR="Na" VALUE="0" />
      <E VAR="p" VALUE="-2*kinvis*(x-1)" />

    </FUNCTION>
    
  </CONDITIONS>

  <GEOMETRY DIM="2" SPACE="2">
    
    <VERTEX>
      <!-- Always must have four values per entry. -->      
      <V ID="0"> 0.0    0.0    0.0 </V>
      <V ID="1"> 0.5    0.0    0.0 </V>
      <V ID="2"> 1.0    0.0    0.0 </V>
      <V ID="3"> 0.0    0.5    0.0 </V>
      <V ID="4"> 0.5    0.5    0.0 </V>
      <V ID="5"> 1.0    0.5    0.0 </V>
      <V ID="6"> 0.0    1.0    0.0 </V>
      <V ID="7"> 0.5    1.0    0.0 </V>
      <V ID="8"> 1.0    1.0    0.0 </V>
    </VERTEX>

    <EDGE>
      <E ID="0"> 0 1 </E>
      <E ID="1"> 1 2 </E>
      <E ID="2"> 0 3 </E>
      <E ID="3"> 1 4 </E>
      <E ID="4"> 2 5 </E>
      <E ID="5"> 3 4 </E>
      <E ID="6"> 4 5 </E>
      <E ID="7"> 3 6 </E>
      <E ID="8"> 4 7 </E>
      <E ID="9"> 5 8 </E>
      <E ID="10"> 6 7 </E>
      <E ID="11"> 7 8 </E>
    </EDGE>
    
    <!-- Q - quads, T - triangles, S - segments, E - tet, P - pyramid, R - prism, H - hex -->
    <!-- Only certain element types are appropriate for the given dimension (dim on mesh) -->
    <!-- Can also use faces to define 3-D elements.  Specify with F[1] for face 1, for example. -->
    <ELEMENT>
      <Q ID="0"> 0 3 5 2 </Q>
      <Q ID="1"> 1 4 6 3 </Q>
      <Q ID="2"> 5 8 10 7 </Q>
      <Q ID="3"> 6 9 11 8 </Q>
    </ELEMENT>
    
    <COMPOSITE>
      <C ID="0"> Q[0-3] </C>
      <C ID="1"> E[0,1,10,11] </C>   // Walls
      <C ID="2"> E[2,7] </C>         // Inflow
      <C ID="3"> E[4,9] </C>         // Outflow
    </COMPOSITE>

    <DOMAIN> C[0] </DOMAIN>

  </GEOMETRY>
  
</NEKTAR>

